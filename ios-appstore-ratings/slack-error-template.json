{
  "text": "iOS App Store Ratings (US)",
  "blocks": [
    {
      "type": "header",
      "text": {
        "type": "plain_text",
        "text": ":x: :firefox: :testops-apple: iOS Appstore Rating (US)",
        "emoji": true
      }
    }
  ],
  "attachments": [
    {
      "color": "red",
      "blocks": [
        {
          "type": "table",
          "rows": [
            [
              {
                "type": "rich_text",
                "elements": [
                  {
                    "type": "rich_text_section",
                    "elements": [
                      { "type": "text", "text": "App", "style": { "bold": true } }
                    ]
                  }
                ]
              },
              {
                "type": "rich_text",
                "elements": [
                  {
                    "type": "rich_text_section",
                    "elements": [
                      {
                        "type": "text",
                        "text": "Current Rating",
                        "style": { "bold": true }
                      }
                    ]
                  }
                ]
              },
              {
                "type": "rich_text",
                "elements": [
                  {
                    "type": "rich_text_section",
                    "elements": [
                      {
                        "type": "text",
                        "text": "Last Rating",
                        "style": { "bold": true }
                      }
                    ]
                  }
                ]
              },
              {
                "type": "rich_text",
                "elements": [
                  {
                    "type": "rich_text_section",
                    "elements": [
                      { "type": "text", "text": "Reviews", "style": { "bold": true } }
                    ]
                  }
                ]
              },
              {
                "type": "rich_text",
                "elements": [
                  {
                    "type": "rich_text_section",
                    "elements": [
                      { "type": "text", "text": "Version", "style": { "bold": true } }
                    ]
                  }
                ]
              }
            ],
            [
              { "type": "raw_text", "text": "${firefox_name}" },
              { "type": "raw_text", "text": "${firefox_rating_text}" },
              { "type": "raw_text", "text": "${firefox_rating_overall_text}" },
              { "type": "raw_text", "text": "${string(firefox_count)}" },
              { "type": "raw_text", "text": "${firefox_version}" }
            ]
          ]
        }
      ]
    }
  ]
}